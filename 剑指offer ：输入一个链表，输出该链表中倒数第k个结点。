struct ListNode* FindKthToTail(struct ListNode* pListHead, int k ) {
    // write code here
    struct ListNode*fast=pListHead,*slow=pListHead;
    while(k--)
    {
        if(fast ==NULL)
        {
            return NULL;
        }
        else
        {
            fast=fast->next;
        }
    }
    while(fast)
    {
        fast=fast->next;
        slow=slow->next;
    }
    return slow;
}
